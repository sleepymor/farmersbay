<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="mainPane" prefHeight="560.0" prefWidth="940.0" xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="projects.farmersbay.view.Admin.Dashboard">
    <children>
        <Pane prefHeight="560.0" prefWidth="940.0">
            <children>
                <Pane prefHeight="560.0" prefWidth="234.0" style="-fx-background-color: #00bc01;">
                    <children>
                        <Text fx:id="Logo" layoutX="68.0" layoutY="47.0" fill="WHITE" stroke="WHITE"
                              strokeType="OUTSIDE" text="Farmers Bay" textAlignment="CENTER">
                            <font>
                                <Font name="System Bold" size="17.0"/>
                            </font>
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Text>
                        <Text fx:id="Logo1" layoutX="74.0" layoutY="70.0" fill="WHITE" stroke="WHITE"
                              strokeType="OUTSIDE" text="Dashboard" textAlignment="CENTER">
                            <font>
                                <Font name="System Bold" size="17.0"/>
                            </font>
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Text>
                        <Pane fx:id="Itemlist" layoutX="30.0" layoutY="105.0" prefHeight="430.0" prefWidth="175.0"
                              style="-fx-background-color: #ffffff;">
                            <children>
                                <Button fx:id="additem" layoutX="18.0" layoutY="14.0" mnemonicParsing="false"
                                        prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: #00bc01;"
                                        text="Add Item" textFill="WHITE" onMouseClicked="#handleLoadCrud">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="deleteitem" layoutX="18.0" layoutY="60.0" mnemonicParsing="false"
                                        prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: #00bc01;"
                                        text="Delete Item" onMouseClicked="#handleDeleteItem" textFill="WHITE">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="addstock" layoutX="18.0" layoutY="109.0" mnemonicParsing="false"
                                        prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: #00bc01;"
                                        text="Add Stock" textFill="WHITE" onMouseClicked="#handleAddStock" >
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="addcategory" layoutX="18.0" layoutY="156.0" mnemonicParsing="false"
                                        prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: #00bc01;"
                                        text="Add/Edit Category" textFill="WHITE" onMouseClicked="#handleCategoryCrud">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="Logout" layoutX="20.0" layoutY="373.0" mnemonicParsing="false"
                                        prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: #00bc01;"
                                        text="Log Out" textFill="WHITE" onMouseClicked="#handleLogoutClick">
                                    <font>
                                        <Font name="System Bold" size="13.0"/>
                                    </font>
                                </Button>
                            </children>
                        </Pane>

                        <Line layoutX="117.0" layoutY="84.0" startX="-100.0" endX="100.0" stroke="WHITE"/>
                    </children>
                </Pane>
                <Pane layoutX="234.0" prefHeight="560.0" prefWidth="707.0">
                    <children>
                        <Pane fx:id="Tabel" layoutX="44.0" layoutY="27.0" prefHeight="488.0" prefWidth="620.0"
                              style="-fx-background-color:rgb(197, 193, 193);"/>
                    </children>
                </Pane>
            </children>
        </Pane>
    </children>
</VBox>
